{
  "name": "inqoil",
  "version": "0.1.0-beta.0",
  "private": false,
  "description": "Inqoil file uploader. Currently supports Supabase Storage Upload",
  "keywords": [
    "react",
    "uploader",
    "supabase",
    "storage"
  ],
  "author": "Hamdu Napari Salifu",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@supabase/supabase-js": "^1.35.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "encoding": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.7",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
